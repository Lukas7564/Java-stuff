
public class upgift2 {

    public static void main(String args[]) {
    float num1 = 5;
    float num2 = 7;
        System.out.println(num1);
        System.out.println(num2);
        System.out.println(num1+num2);
        System.out.println(num1-num2);
        System.out.println(num1*num2);
        System.out.println(num1/num2);

    }

}
